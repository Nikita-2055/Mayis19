#include<stdio.h>
int function(int *arr, int count){
    for(int i = 0; i < 8; i++){
        if(arr[i] % 2 == 1){
            count++;
        }
    }
    return count;
}
int main(){
    int arr[] = {1,2,4,5,6,7,8,9};
    int count = 0;
    printf("in array have odd: %d\n",function(arr,count));
    return 0;
}
